#include <stdio.h>
#include "dog.h"

/**
 * init_dog - function to initialize a struct variable
 *
 *
 *Return: void
 */

void init_dog(struct dog *d, char *name, float age, char *owner)

{
	struct dog{
	char *name;
	float age;
	char *owner;
	};

	struct dog p;
	struct dog *ptr;

	ptr = &dog;
	(*ptr).name = *name;
	(*ptr).age = age;
	(*ptr).owner = *owner;
}
